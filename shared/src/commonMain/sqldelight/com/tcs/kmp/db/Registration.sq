CREATE TABLE person(
PID TEXT PRIMARY KEY NOT NULL,
NAME TEXT NOT NULL,
EMAIL TEXT NOT NULL,
MOBILE TEXT NOT NULL,
PASSWORD TEXT NOT NULL,
GENDER TEXT NOT NULL
);

insertPerson:
INSERT OR REPLACE INTO person(PID, NAME,EMAIL,MOBILE,PASSWORD,GENDER)
VALUES (?, ?,?,?,?,?);

getAllPersons:
SELECT * FROM person;

removeAllPersons:
DELETE FROM person;